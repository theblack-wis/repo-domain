{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.5.0"
    }
  },
  "routes": [
    { "src": "/data/(.*)", "dest": "/api/data/$1" },
    { "src": "/(.*)", "dest": "/api/$1" }
  ],
  "env": {
    "NOW_PHP_FOO": "bar3"
  },
  "build": {
    "env": {
      "NOW_PHP_DEBUG": "1"
    }
  }
}
